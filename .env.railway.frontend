# Railway Frontend Service Environment Variables
# Copy these to your Railway frontend service settings

# Environment
NODE_ENV=production
PORT=3000

# API Configuration
VITE_API_URL=https://your-backend.railway.app

# Clerk Authentication (Public Key)
VITE_CLERK_PUBLISHABLE_KEY=pk_live_your_clerk_publishable_key_here

# Application Settings
VITE_APP_NAME=Amazon DSP OAuth Manager
VITE_APP_VERSION=1.0.0

# Feature Flags (optional)
VITE_ENABLE_DSP_SEATS=true
VITE_ENABLE_AMC=true
VITE_ENABLE_ANALYTICS=false

# Monitoring (optional)
VITE_SENTRY_DSN=
VITE_GA_TRACKING_ID=